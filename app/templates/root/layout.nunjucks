<!doctype html>
<html class="no-js" lang="">
  <head>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Wet Surface Decals</title>

    <link rel='shortcut icon' href='/images/placeholder_icon.png' type='image/x-icon' >
    <link rel="apple-touch-icon" href="/images/placeholder_icon.png">

    <!-- build:css /styles/vendor.css -->
    <!-- bower:css -->
    <!-- endbower -->
    <!-- endbuild -->

    <style>
        {% block page_style %}{% endblock %}
    </style>

    {% block head %}{% endblock %}

    <!--
        Roboto: Main body font
    -->
    <link href='https://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>

    <!-- build:css /styles/root/main.css -->
    <link rel="stylesheet" href="styles/root/main.css">
    <link rel="stylesheet" href="/bower_components/jquery-eu-cookie-law-popup/css/jquery-eu-cookie-law-popup.css">
    <!-- endbuild -->
  </head>
  <body class="{{ body_class }} eupopup">

    <!--[if lt IE 10]>
      <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->

    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

          <ul class="nav navbar-nav navbar-right">

            <li><a class="navbar-brand dissonance hidden-xs" href="/dissonance" target="_blank">
              {% include "images/tint.svg" %}
            </a></li>
            <li><a class="navbar-brand wetsurfacedecals hidden-xs" href="/wetdurfacedecals" target="_blank">
              {% include "images/tint.svg" %}
            </a></li>
            <li><a class="navbar-social navbar-brand twitter hidden-xs" href="https://twitter.com/PlaceholderSoft" target="_blank">
              {% include "images/twitter.svg" %}
            </a></li>
            <li><a class="navbar-social navbar-brand github hidden-xs" href="https://twitter.com/PlaceholderSoft" target="_blank">
              {% include "images/github.svg" %}
            </a></li>

            <li><a href="/dissonance" target="_blank" class="visible-xs">Dissonance Voice Chat</a></li>
            <li><a href="/wetsurfacedecals" target="_blank" class="visible-xs">Wet Surface Decals</a></li>
            <li><a href="https://github.com/Placeholder-Software" target="_blank" class="visible-xs">GitHub</a></li>
            <li><a href="https://twitter.com/PlaceholderSoft" target="_blank" class="visible-xs">Twitter</a></li>
          </ul>

        </div>
      </div>
    </nav>

    <header class="site-title-container">
      <div class="container-fluid">
        <a href="/" class="row site-title-row">
          {% include "images/Placeholder Header.svg" %}
        </a>
      </div>
    </header>

    <div class="container main-content">

      <!-- You write code for this content block in another file -->
      {% block content %} {% endblock %}

    </div>

    <footer class="navbar">
      <div class="container">
        <div class="row underline-links">
          <div class="col-xs-6 col-sm-4">
            <h4>Connect</h4>
            <ul class="footer-list">
              <li><a href="mailto:admin@placeholder-software.co.uk">Contact Us</a></li>
              <li><a href="https://twitter.com/placeholdersoft">Twitter</a></li>
            </ul>
          </div>
          <div class="col-xs-6 col-sm-4">
            <h4>Resources</h4>
            <ul class="footer-list">
              <li><a href="https://dissonance.readthedocs.io/en/latest/" target="_blank">Documentation</a></li>
              <li><a href="https://assetstore.unity.com/publishers/23373" target="_blank">Asset Store</a></li>
              <li><a href="https://www.youtube.com/channel/UCZXN3n6Etm7wgwkSYiuP6jA">YouTube</a></li>
              <li><a href="/">Placeholder Software</a></li>
            </ul>
          </div>
          <div class="col-xs-12 col-sm-4">
            <h4>Site</h4>
            <p class="footer-list">
              Site built by <a href="http://martindevans.me/" target="_blank">Martin Evans</a> with <a href="https://getbootstrap.com/" target="_blank">Bootstrap</a>.
              Source <a href="https://github.com/Placeholder-Software/placeholder-software.co.uk" target="_blank">available</a> under MIT license.
            </p>
          </div>
        </div>
    </footer>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-46722926-1', 'auto');
      ga('send', 'pageview');
    </script>

    <!-- build:js /scripts/vendor.js -->
    <!-- bower:js -->
    <script src="/bower_components/jquery/dist/jquery.js"></script>
    <script src="/bower_components/jquery-eu-cookie-law-popup/js/jquery-eu-cookie-law-popup.js"></script>
    <!-- endbower -->
    <!-- endbuild -->

    <!-- build:js /scripts/plugins.js -->
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/affix.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/alert.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/dropdown.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/modal.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/transition.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/button.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/popover.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/carousel.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/scrollspy.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/collapse.js"></script>
    <script src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/tab.js"></script>
    <!-- endbuild -->

    <!-- build:js /scripts/main.js -->
    <script src="/app/scripts/main.js"></script>
    <!-- endbuild -->
    
    {% block end_body_script %}{% endblock %}
  </body>
</html>
